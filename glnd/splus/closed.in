      group=base_pat_char2[,"treatment"],
      caption="Patient Characteristics (Continued)", 
      append=T, zero=F, vpvalue=F,vn=T,vmean=T,vmedian=T,vcut=F,vrange=T,vdigits=1,total=T)
      
      
status <- f.sas.get(library=lib,member="status",format.library=libs,formats=T)
f.tab(file="closed", x=status[,c("days.hosp","days.hosp.post.entry", "ever.on.ventilation.study"), drop=F], group=status[,"treatment"],
      caption="Duration of Hospitalization", 
      append=T, zero=F, vpvalue=F,vn=T,vmean=T,vmedian=T,vcut=F,vrange=T,vdigits=1,total=T)    


cat("\\clearpage",sep="\n",file="closed.tex",append=T)



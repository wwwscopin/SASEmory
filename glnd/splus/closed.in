### Baseline patient characteristics 

base_pat_char2 <- f.sas.get(library=lib,member="basedemo", format.library=libs,formats=T)
f.tab(file="closed",   x=base_pat_char2[,c("gender","rac","hispanic","affil","apache.id", "apachese", "ards","mech.vent","age"), drop=F],  
    group=base_pat_char2[,"treatment"],
    footnote="mad - median of the absolute values of the deviation from the median",
    caption="Patient Characteristics", 
    append=T, zero=F, vpvalue=F,vn=T,vmean=T,vmedian=T,vcut=F,vrange=T,vdigits=1,total=T)
      
    cat("\\clearpage",sep="\n",file="closed.tex",append=T)
      
           
f.tab(file="closed",  x=base_pat_char2[,c("bmi","surg","diag"),  drop=F], group=base_pat_char2[,"treatment"],
     caption="Patient Characteristics (Continued)", 
     append=T, zero=F, vpvalue=F,vn=T,vmean=T,vmedian=T,vcut=F,vrange=T,vdigits=1,total=T)
     
     
     cat("\\clearpage",sep="\n",file="closed.tex",append=T)
     
     
f.tab(file="closed",    x=base_pat_char2[,c("daysop","int.aortic.pump","nosc.infect","nutr.status","wbc.count"),   drop=F],
     group=base_pat_char2[,"treatment"],
     caption="Patient Characteristics (Continued)", 
     append=T, zero=F, vpvalue=F,vn=T,vmean=T,vmedian=T,vcut=F,vrange=T,vdigits=1,total=T)
      
      
 cat("\\clearpage",sep="\n",file="closed.tex",append=T)
 
 
 f.tab(file="closed", 
      x=base_pat_char2[,c("indication.pn.1","indication.pn.2","indication.pn.3",
